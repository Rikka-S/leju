<view class="data-v-69590f4b"><view class="title data-v-69590f4b">等待支付</view><navigator class="select-address data-v-69590f4b" url="../../user/address/address"><view class="data-v-69590f4b"><text class="data-v-69590f4b">{{"姓名："+address.name}}</text><text class="data-v-69590f4b">{{"电话号码："+address.phoneNumber}}</text></view><view class="data-v-69590f4b"><text class="data-v-69590f4b">{{"收获地址："+address.province+"-"+address.city+"-"+address.region+"-"+address.detailAddress}}</text></view></navigator><view class="order-info data-v-69590f4b"><view class="header data-v-69590f4b"><text class="data-v-69590f4b">{{"订单编号:"+orderBase.orderSn}}</text><text class="data-v-69590f4b">等待支付</text></view><block wx:for="{{orderItems}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="order-main data-v-69590f4b"><image src="{{item.productPic}}" mode class="data-v-69590f4b"></image><view class="text data-v-69590f4b"><view class="name-price data-v-69590f4b"><text class="data-v-69590f4b">{{item.productName}}</text><text class="data-v-69590f4b">{{"￥"+item.productPrice+"元"}}</text></view><view class="id-num data-v-69590f4b"><text class="data-v-69590f4b">{{"编号:"+item.productSkuId}}</text><text class="data-v-69590f4b">{{"x"+item.productQuantity}}</text></view><view class="color data-v-69590f4b">规格:<block wx:for="{{item.productAttr}}" wx:for-item="el" wx:for-index="index" wx:key="index"><text class="data-v-69590f4b">{{el.key+":"+el.value}}</text></block></view></view></view></block><view class="total data-v-69590f4b">{{'合计：'+total+'元'}}</view><view class="btns data-v-69590f4b"><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="btn data-v-69590f4b" bindtap="__e">确认付款</view></view></view></view>